# The ocaml subsystem defines the following modules:
# - ocaml.prog -- Prepare OCaml programs
# - ocaml.lib -- Prepare OCaml libraries
# - ocaml.pack -- Prepare packed OCaml libraries
# - ocaml.meta -- Prepare OCaml findlib META files
# - ocaml.toplevel -- Prepare OCaml toplevels

SHARE+=			ocaml.uses.mk
SHARE+=			ocaml.module.mk
SHARE+=			ocaml.external.mk
SHARE+=			ocaml.build.mk
SHARE+=			ocaml.depend.mk
SHARE+=			ocaml.find.mk
SHARE+=			ocaml.meta.mk
SHARE+=			ocaml.init.mk
SHARE+=			ocaml.lex.mk
SHARE+=			ocaml.lib.mk
SHARE+=			ocaml.pack.mk
SHARE+=			ocaml.main.mk
SHARE+=			ocaml.object.mk
SHARE+=			ocaml.prog.mk
SHARE+=			ocaml.dirs.mk
SHARE+=			ocaml.source.mk
SHARE+=			ocaml.compile.mk
SHARE+=			ocaml.tools.mk
SHARE+=			ocaml.yacc.mk
SHARE+=			ocaml.odoc.mk
SHARE+=			ocaml.manual.mk
SHARE+=			ocaml.toplevel.mk

.include "generic.files.mk"

